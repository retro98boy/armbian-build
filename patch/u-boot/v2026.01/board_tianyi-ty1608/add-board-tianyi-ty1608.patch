index 00000000..dd1c3f20
--- /dev/null
+++ b/dts/upstream/src/arm64/amlogic/meson-gxl-s905x-tianyi-ty1608.dts
@@ -0,0 +1,170 @@
+// SPDX-License-Identifier: (GPL-2.0+ OR MIT)
+/*
+ * Copyright (c) 2025 retro98boy <retro98boy@qq.com>
+ */
+
+/dts-v1/;
+
+#include "meson-gxl-s905x-p212.dtsi"
+#include <dt-bindings/sound/meson-aiu.h>
+#include <dt-bindings/input/input.h>
+#include <dt-bindings/leds/common.h>
+
+/ {
+	compatible = "tianyi,ty1608", "amlogic,s905x", "amlogic,meson-gxl";
+	model = "Tianyi TY1608";
+
+	memory@0 {
+		reg = <0x0 0x0 0x0 0x40000000>;
+	};
+
+	gpio-leds {
+		compatible = "gpio-leds";
+
+		/*
+		 * The power LED on the device is always on, defaulting to red.
+		 * Pulling GPIODV_24 high changes it to green, indicating that the kernel has started.
+		 */
+		pwr-led-green {
+			function = LED_FUNCTION_POWER;
+			color = <LED_COLOR_ID_GREEN>;
+			gpios = <&gpio GPIODV_24 GPIO_ACTIVE_HIGH>;
+			default-state = "on";
+		};
+
+		/*
+		 * modprobe ledtrig-netdev
+		 * echo netdev > /sys/class/leds/green\:lan/trigger
+		 * echo wlan0 > /sys/class/leds/green\:lan/device_name
+		 *
+		 * echo 1 > /sys/class/leds/green\:lan/link
+		 * or
+		 * echo 1 > /sys/class/leds/green\:lan/rx
+		 */
+		lan-led-green {
+			function = LED_FUNCTION_LAN;
+			color = <LED_COLOR_ID_GREEN>;
+			gpios = <&gpio GPIODV_27 GPIO_ACTIVE_HIGH>;
+			default-state = "off";
+			linux,default-trigger = "netdev";
+		};
+
+		ir-led-green {
+			function = LED_FUNCTION_STATUS;
+			color = <LED_COLOR_ID_GREEN>;
+			gpios = <&gpio GPIODV_26 GPIO_ACTIVE_HIGH>;
+			default-state = "on";
+		};
+	};
+
+	dio2128: analog-amplifier {
+		compatible = "simple-audio-amplifier";
+		sound-name-prefix = "AU2";
+		VCC-supply = <&hdmi_5v>;
+		enable-gpios = <&gpio GPIOH_5 GPIO_ACTIVE_HIGH>;
+	};
+
+	cvbs-connector {
+		compatible = "composite-video-connector";
+
+		port {
+			cvbs_connector_in: endpoint {
+				remote-endpoint = <&cvbs_vdac_out>;
+			};
+		};
+	};
+
+	sound {
+		compatible = "amlogic,gx-sound-card";
+		model = "tianyi-ty1608";
+		audio-aux-devs = <&dio2128>;
+		audio-widgets = "Line", "Lineout";
+		audio-routing = "AU2 INL", "ACODEC LOLN",
+				"AU2 INR", "ACODEC LORN",
+				"Lineout", "AU2 OUTL",
+				"Lineout", "AU2 OUTR";
+		clocks = <&clkc CLKID_MPLL0>,
+			 <&clkc CLKID_MPLL1>,
+			 <&clkc CLKID_MPLL2>;
+
+		assigned-clocks = <&clkc CLKID_MPLL0>,
+				  <&clkc CLKID_MPLL1>,
+				  <&clkc CLKID_MPLL2>;
+		assigned-clock-parents = <0>, <0>, <0>;
+		assigned-clock-rates = <294912000>,
+				       <270950400>,
+				       <393216000>;
+		dai-link-0 {
+			sound-dai = <&aiu AIU_CPU CPU_I2S_FIFO>;
+		};
+
+		dai-link-1 {
+			sound-dai = <&aiu AIU_CPU CPU_I2S_ENCODER>;
+			dai-format = "i2s";
+			mclk-fs = <256>;
+
+			codec-0 {
+				sound-dai = <&aiu AIU_ACODEC CTRL_I2S>;
+			};
+		};
+
+		dai-link-2 {
+			sound-dai = <&aiu AIU_ACODEC CTRL_OUT>;
+
+			codec-0 {
+				sound-dai = <&acodec>;
+			};
+		};
+	};
+};
+
+&acodec {
+	AVDD-supply = <&vddio_ao18>;
+	status = "okay";
+};
+
+&aiu {
+	status = "okay";
+};
+
+&cvbs_vdac_port {
+	cvbs_vdac_out: endpoint {
+		remote-endpoint = <&cvbs_connector_in>;
+	};
+};
+
+&hdmi_5v {
+	gpio = <&gpio GPIOAO_4 GPIO_ACTIVE_HIGH>;
+};
+
+&sd_emmc_a {
+	status = "disabled";
+};
+
+&sd_emmc_b {
+	status = "disabled";
+};
+
+&uart_A {
+	status = "disabled";
+	/delete-node/ bluetooth;
+};
+
+&usb {
+	dr_mode = "host";
+};
+
+/* https://github.com/chewitt/linux/blob/amlogic-6.18.y/arch/arm64/boot/dts/amlogic/meson-gxlx-s905l-p271.dts */
+/* Mali 450-MP2 */
+&mali {
+	interrupts = <GIC_SPI 160 IRQ_TYPE_LEVEL_HIGH>,
+		     <GIC_SPI 161 IRQ_TYPE_LEVEL_HIGH>,
+		     <GIC_SPI 162 IRQ_TYPE_LEVEL_HIGH>,
+		     <GIC_SPI 163 IRQ_TYPE_LEVEL_HIGH>,
+		     <GIC_SPI 164 IRQ_TYPE_LEVEL_HIGH>,
+		     <GIC_SPI 165 IRQ_TYPE_LEVEL_HIGH>,
+		     <GIC_SPI 166 IRQ_TYPE_LEVEL_HIGH>,
+		     <GIC_SPI 167 IRQ_TYPE_LEVEL_HIGH>;
+	interrupt-names = "gp", "gpmmu", "pp", "pmu",
+		"pp0", "ppmmu0", "pp1", "ppmmu1";
+};
